{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "extends": "airbnb",
  "rules": {
    "semi": 2,
    "space-before-function-paren": 0,
    "react/prefer-stateless-function": 0,
    "comma-dangle": 0,
    "object-curly-spacing": ["error", "never"],
    "func-names": 0,
    "arrow-body-style": 0
  }
}